<div class="container">
<h2>Sign up</h2>
<h5>Hint: all fields required</h5>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :first_name %>
  <%= f.text_field :first_name, autofocus: true %></div>

  <div><%= f.label :last_name %>
  <%= f.text_field :last_name, autofocus: true %></div>

  <div><%= f.label :email %>
  <%= f.email_field :email, autofocus: true %></div>

  <div><%= f.label :track %>
  <%= f.select :track, [['Product and Design','Product and Design'],['Sales','Sales'],['Technical Marketing','Technical Marketing'],['Web Development','Web Development'],] %>

  <div><%= f.label :photo %>
  <%= f.file_field :photo, :class => 'file_field' %>

  <p></p>

  <div><%= f.label :password %>
    <%= f.password_field :password, autocomplete: "off" %></div>

  <div><%= f.label "Password (confirm)" %>
    <%= f.password_field :password_confirmation, autocomplete: "off" %></div>

  <p></p>

  <div><%= f.label :access_code %>
  <%= f.text_field :access_code, autocomplete: "off" %></div>

  <p></p>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<p></p>
<%= link_to "Home", root_path %>
<p></p>

<%= render "devise/shared/links" %>
</div>
